list=[10,20,30,9]
sum=59
list_size=len(list)


for i in range(0, list_size-2):
    for j in range(i+1, list_size-1):
        for k in range(j+1, list_size):
            if list[i]+list[j]+list[k]==sum:
                print("triplet is ", list[i],list[j],list[k])
                
            